<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bluec.delivery_homework2.DeliveryDriver">

    <Button
        android:id="@+id/AddDelivery"
        android:layout_width="158dp"
        android:layout_height="56dp"
        android:layout_marginBottom="364dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:text="@string/Add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/RemoveDelivery"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.416" />

    <Button
        android:id="@+id/RemoveDelivery"
        android:layout_width="156dp"
        android:layout_height="57dp"
        android:layout_marginBottom="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="21dp"
        android:text="@string/Remove"
        app:layout_constraintBottom_toTopOf="@+id/Paste"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/AddDelivery" />

    <Button
        android:id="@+id/Cut"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="13dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="33dp"
        android:text="@string/Cut"
        app:layout_constraintBottom_toTopOf="@+id/Head"
        app:layout_constraintEnd_toStartOf="@+id/Paste"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddDelivery" />

    <Button
        android:id="@+id/Paste"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="13dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="31dp"
        android:text="@string/Paste"
        app:layout_constraintBottom_toTopOf="@+id/Tail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Cut"
        app:layout_constraintTop_toBottomOf="@+id/AddDelivery" />

    <Button
        android:id="@+id/Head"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="13dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="11dp"
        android:text="@string/Head"
        app:layout_constraintBottom_toTopOf="@+id/Forward"
        app:layout_constraintEnd_toStartOf="@+id/Tail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Cut" />

    <Button
        android:id="@+id/Tail"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="13dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="13dp"
        android:text="@string/Tail"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Head"
        app:layout_constraintTop_toBottomOf="@+id/Paste" />

    <Button
        android:id="@+id/Forward"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="11dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="13dp"
        android:text="@string/Forward"
        app:layout_constraintBottom_toTopOf="@+id/Switch"
        app:layout_constraintEnd_toStartOf="@+id/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Head" />

    <Button
        android:id="@+id/back"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="11dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="13dp"
        android:text="@string/Back"
        app:layout_constraintBottom_toTopOf="@+id/Print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Forward"
        app:layout_constraintTop_toBottomOf="@+id/Tail" />

    <Button
        android:id="@+id/Switch"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="83dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="11dp"
        android:text="@string/Switch"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Print"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Forward" />

    <Button
        android:id="@+id/Print"
        android:layout_width="156dp"
        android:layout_height="56dp"
        android:layout_marginBottom="83dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="11dp"
        android:text="@string/Print"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Switch"
        app:layout_constraintTop_toBottomOf="@+id/back" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="267dp"
        android:layout_height="31dp"
        android:layout_marginEnd="96dp"
        android:layout_marginStart="97dp"
        android:layout_marginTop="16dp"
        android:text="@string/Hello"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/AddDelivery"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/Quit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/Quit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>
